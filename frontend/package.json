		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"sass": "1.86.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess-markdown": "^2.7.3",
